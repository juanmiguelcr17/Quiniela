@model IEnumerable<QuinielaMVC4.Models.Entities.Season>

@{
    ViewBag.Title = "Quiniela | Temporadas";
}

<p>
    @Html.ActionLink("Nueva Temporada", "Create")
</p>

<table class="content">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Year)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Starts)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ends)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Weeks)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Year)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Starts)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ends)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Weeks)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.SeasonId }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.SeasonId }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.SeasonId })
        </td>
    </tr>
}

</table>
